    {
     "when": {
    "facing":"south"
    },
      "apply": {
        "model": "urushi:block/concrete_slab_wall_normal",
         "y": 180
      }
    },
    {
     "when": {
    "facing":"west"
    },
      "apply": {
        "model": "urushi:block/concrete_slab_wall_normal",
         "y": 180
      }
    }
  ]
}